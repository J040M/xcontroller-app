{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "xcontroller-app",
  "mainBinaryName": "xcontroller-app",
  "version": "0.3.2",
  "identifier": "io.j040m.xcontrollerapp",
  "plugins": {},
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "xcontroller",
        "width": 800,
        "height": 600,
        "useHttpsScheme": true
      }
    ]
  }
}